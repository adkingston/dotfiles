(function( $ ) {
	'use strict';

  $(function() {
    if (typeof Cookies.get('accept_cookies') == 'undefined') {
      $('.cookie-notice').fadeIn();
    }
    $('button.accept').on('click', function(){
      Cookies.set('accept_cookies', 1, { expires: 7 });
      $('.cookie-notice').fadeOut();
    })

    $('.cookie-notice .close-button').on('click', function(){
      Cookies.set('accept_cookies', 0, { expires: 7 });
      $('.cookie-notice').fadeOut();
    })
  });

})( jQuery );
